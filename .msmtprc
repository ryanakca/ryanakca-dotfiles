defaults
logfile ~/.msmtp.log
domain LOCALHOST

account default
host smtp.gmail.com
port 587
from rak@debian.org
tls on
tls_starttls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
tls_min_dh_prime_bits 1024
auth on
user ryanakca@gmail.com
password GMAIL_PASS

account queensu
host mail.queensu.ca
port 465
tls on
tls_starttls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
tls_certcheck on
auth on
user 9rak
password QUEENSU_PASS

