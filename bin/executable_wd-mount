#!/bin/sh

case `uname` in
    OpenBSD)
        doas bioctl -c C -l fe3fd8d53b06049b.a softraid0
        doas mount /media/wd
        ;;
    Linux)
        if ! zpool list wd-pass > /dev/null 2>&1 ; then
            sudo zpool import -l wd-pass
        fi
        ;;
    *)
        echo "Unknown host"
        exit 1
esac

# vim: set sw=4:
